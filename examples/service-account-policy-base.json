{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:DeleteObject",
                "s3:PutObject",
                "s3:PutObjectTagging",
                "s3:GetObjectTagging",
                "s3:DeleteObjectTagging"
            ],
            "Resource": [
                "arn:aws:s3:::BUCKET_NAME_REPLACE_ME",
                "arn:aws:s3:::BUCKET_NAME_REPLACE_ME/*"
            ]
        }
    ]
}
