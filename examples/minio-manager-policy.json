{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
              "s3:GetBucketLocation",
              "s3:GetBucketVersioning",
              "s3:GetObject",
              "s3:GetObjectRetention",
              "s3:GetObjectVersion",
              "s3:ListAllMyBuckets",
              "s3:ListBucket",
              "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::minio-manager-secrets",
                "arn:aws:s3:::minio-manager-secrets/*"
            ]
        }
    ]
}
